@model IEnumerable<AtelierPratique_16.Models.billet>

@{
    ViewBag.Title = "Index";
}

<h2>Billets</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date_Delivrance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.voyage.Ville_Depart)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date_Delivrance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.voyage.Ville_Depart)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.N_Billet }) |
            @Html.ActionLink("Details", "Details", new { id=item.N_Billet }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.N_Billet })
        </td>
    </tr>
}

</table>
